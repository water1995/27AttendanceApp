<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abc.a27attendanceapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".prof_create_class"></activity>
        <activity
            android:name=".stud_home"
            android:label="@string/title_activity_stud_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".prof_home"
            android:label="@string/title_activity_prof_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".stud_register" />
        <activity android:name=".register_Professor" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>